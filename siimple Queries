SELECT *
FROM log_in_attempts
ORDER BY login_date DESC;


SELECT firstname, lastname, title, email
FROM employees
WHERE title = 'IT Staff';


select *
    -> from log_in_attempts
    -> where login_time > "18:00:00" and success = 0;
